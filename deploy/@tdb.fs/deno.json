{
  "name": "@tdb/public-fs",
  "version": "0.0.50",
  "license": "MIT",
  "tasks": {
    "test": "deno test -P=testset",
    "build": "deno run -P=build ./-scripts/-build.ts && deno task help",
    "serve": "deno run -P=serve ./-scripts/-serve.ts",
    "deploy": "deno task build && orbiter deploy && deno task help",
    "clean": "deno run -P=clean ./-scripts/-clean.ts",
    "dry": "deno publish --allow-dirty --dry-run",
    "check": "deno check src/*",
    "help": "deno run -P=dev ./-scripts/-help.ts",
    "tmp": "deno run -A ./-scripts/-tmp.ts"
  },
  "exports": {
    ".": "./src/mod.ts",
    "./t": "./src/types.ts",
    "./types": "./src/types.ts"
  }
}
