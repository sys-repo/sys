{
  "name": "@sys/http",
  "version": "0.0.114",
  "license": "MIT",
  "tasks": {
    "dry": "deno publish --allow-dirty --dry-run",
    "clean": "deno run -P=clean ./-scripts/-clean.ts",
    "test": "deno test -P=testset",
    "serve": "deno run -P=serve @sys/http/serve --cmd=start --port=1234 --dir='./src/-test/-sample.dist'",
    "tmp": "deno run -A ./-scripts/-tmp.ts"
  },
  "exports": {
    ".": "./src/mod.ts",
    "./t": "./src/types.ts",
    "./types": "./src/types.ts",
    "./client": "./src/http.client/mod.ts",
    "./server": "./src/http/mod.ts",
    "./serve": "./src/-serve/mod.ts"
  }
}
