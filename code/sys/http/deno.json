{
  "name": "@sys/http",
  "version": "0.0.98",
  "license": "MIT",
  "tasks": {
    "dry": "deno publish --allow-dirty --dry-run",
    "clean": "deno run -RWE ./-scripts/-clean.ts",
    "test": "deno test -RWNE --allow-run --allow-ffi",
    "serve": "deno run -RNE --allow-run @sys/http/serve --cmd=start --port=1234 --dir='./src/-test/-sample.dist'",
    "tmp": "deno run -A ./-scripts/-tmp.ts"
  },
  "exports": {
    ".": "./src/mod.ts",
    "./t": "./src/types.ts",
    "./types": "./src/types.ts",
    "./client": "./src/m.http.client/mod.ts",
    "./server": "./src/m.http/mod.ts",
    "./serve": "./src/-serve/mod.ts"
  }
}
