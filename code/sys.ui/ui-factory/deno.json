{
  "name": "@sys/ui-factory",
  "version": "0.0.66",
  "license": "MIT",
  "tasks": {
    "dev": "deno run -P=dev   ./-scripts/-vite.ts --cmd=dev    --in=./src/-test/index.html",
    "test": "deno test -P=testset",
    "build": "deno run -P=build ./-scripts/-vite.ts --cmd=build  --in=./src/-test/index.html",
    "serve": "deno run -P=serve ./-scripts/-vite.ts --cmd=serve",
    "dry": "deno publish --allow-dirty --dry-run",
    "check": "deno check src/*",
    "clean": "deno run -P=clean ./-scripts/-clean.ts",
    "help": "deno run -P=dev   ./-scripts/-vite.ts --cmd=help",
    "tmpl": "deno run -P=build ./src/m.tmpl/mod.ts",
    "prep": "deno run -P=build ./src/m.tmpl/mod.ts --bundle",
    "tmp": "deno run -A ./-scripts/-tmp.ts"
  },
  "exports": {
    ".": "./src/mod.ts",
    "./t": "./src/types.ts",
    "./types": "./src/types.ts",
    "./core": "./src/m.core/mod.ts",
    "./schema": "./src/m.schema/mod.ts",
    "./tmpl": "./src/m.tmpl/mod.ts",
    "./adapter/react": "./src/m.react/mod.ts",
    "./components/react": "./src/ui/mod.ts",
    "./sample/catalog": "./src/-test/-sample/catalog.ts",
    "./sample/tmpl": "./-tmpl/react.catalog.folder/mod.ts"
  }
}
