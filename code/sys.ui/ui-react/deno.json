{
  "name": "@sys/ui-react",
  "version": "0.0.0",
  "license": "MIT",
  "tasks": {
    "dev": "deno run -A --node-modules-dir npm:vite --port=1234",
    "build": "deno run -A --node-modules-dir npm:vite build",
    "preview": "deno run -A --node-modules-dir npm:vite preview",
    "serve": "deno run --allow-net --allow-read jsr:@std/http/file-server dist/",
    "test": "deno test -RW",
    "lint": "deno lint",
    "dry": "deno publish --allow-dirty --dry-run"
  },
  "exports": {
    ".": "./src/mod.ts",
    "./t": "./src/types.ts",
    "./types": "./src/types.ts"
  }
}
