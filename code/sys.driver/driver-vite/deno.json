{
  "name": "@sys/driver-vite",
  "version": "0.0.188",
  "license": "MIT",
  "tasks": {
    "dry": "deno publish --allow-dirty --dry-run",
    "check": "deno check src/*",
    "test": "deno test -P=testset",
    "reset": "deno run -P=clean ./-scripts/-reset.ts",
    "init": "deno run -P=dev   ./-scripts/-main.ts --cmd=init    --dir=./.tmp/sample",
    "dev": "deno run -P=dev   ./-scripts/-main.ts --cmd=dev     --dir=./.tmp/sample",
    "build": "deno run -P=build ./-scripts/-main.ts --cmd=build   --dir=./.tmp/sample",
    "serve": "deno run -P=serve ./-scripts/-main.ts --cmd=serve   --dir=./.tmp/sample/dist",
    "clean": "deno run -P=clean ./-scripts/-main.ts --cmd=clean   --dir=./.tmp/sample",
    "backup": "deno run -P=build ./-scripts/-main.ts --cmd=backup  --dir=./.tmp/sample",
    "upgrade": "deno run -P=dev   ./-scripts/-main.ts --cmd=upgrade --dir=./.tmp/sample",
    "help": "deno run -P=dev ./-scripts/-main.ts --cmd=help    --dir=./.tmp/sample",
    "r": "deno task reset && deno task prep && deno task init",
    "rdev": "deno task r && deno task dev",
    "tmp": "deno run -A ./-scripts/-tmp.ts"
  },
  "exports": {
    ".": "./src/mod.ts",
    "./t": "./src/types.ts",
    "./types": "./src/types.ts",
    "./entry": "./src/-entry/mod.ts",
    "./log": "./src/m.log/mod.ts",
    "./main": "./src/-entry/-main.ts",
    "./sample-imports": "./src/-test/-sample-imports.ts"
  }
}
