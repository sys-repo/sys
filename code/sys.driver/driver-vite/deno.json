{
  "name": "@sys/driver-vite",
  "version": "0.0.61",
  "license": "MIT",
  "tasks": {
    "lint": "deno lint",
    "dry": "deno publish --allow-dirty --dry-run",
    "clean": "deno run -RWE ./-scripts/-clean.ts",
    "test": "deno test -RWNE --allow-run --allow-ffi",
    "dev": "deno run -RWNE --allow-run --allow-ffi ./-scripts/-main.ts --cmd=dev --input=./src/-test/vite.sample-2/index.html",
    "build": "deno run -RWE --allow-run --allow-ffi ./-scripts/-main.ts --cmd=build --input=./src/-test/vite.sample-2/index.html",
    "serve": "deno run -RNE --allow-run --allow-ffi ./-scripts/-main.ts --cmd=serve"
  },
  "exports": {
    ".": "./src/mod.ts",
    "./t": "./src/types.ts",
    "./types": "./src/types.ts",
    "./log": "./src/m.Log/mod.ts",
    "./dev": "./src/-entry/-dev.ts",
    "./build": "./src/-entry/-build.ts",
    "./serve": "./src/-entry/-serve.ts"
  }
}
