{
  "version": "0.0.0",
  "tasks": {
    "dev": "deno run -RWNE --allow-run <ENTRY> --cmd=dev",
    "build": "deno run -RWE --allow-run <ENTRY> --cmd=build",
    "serve": "deno run -RNE --allow-run <ENTRY> --cmd=serve",
    "clean": "deno run -RWE <ENTRY> --cmd=clean",
    "upgrade": "deno run -RWNE --allow-run <ENTRY> --cmd=upgrade",
    "backup": "deno run -RWE --allow-run <ENTRY> --cmd=backup",
    "help": "deno run -RE <ENTRY> --cmd=help",
    "sys": "deno run -RWNE <ENTRY_MAIN>",
    "tmp": "deno run -A npm:orbiter-cli"
  },
  "imports": {
    "<SELF_IMPORT_NAME>": "<SELF_IMPORT_URI>",
    "@sys/driver-vite": "jsr:@sys/driver-vite@<VITE_VERSION>"
  }
}
