{
  "name": "@sys/driver-vitepress",
  "version": "0.0.200",
  "license": "MIT",
  "tasks": {
    "lint": "deno lint",
    "dry": "deno publish --allow-dirty --dry-run",
    "clean": "deno run -RWE ./-scripts/-clean.ts",
    "test": "deno test -RWNE --allow-run --allow-ffi",
    "init": "deno run -RWNE --allow-run ./-scripts/-c.init.ts --inDir=./.tmp/sample",
    "dev": "deno run -RWNE --allow-run ./-scripts/-entry.main.ts --cmd=dev --inDir=./.tmp/sample",
    "build": "deno run -RWE --allow-run ./-scripts/-entry.main.ts --cmd=build --inDir=./.tmp/sample",
    "serve": "deno run -RNE --allow-run ./-scripts/-entry.main.ts --cmd=serve --inDir=./.tmp/sample",
    "upgrade": "deno run -RWNE --allow-run ./-scripts/-entry.main.ts --cmd=upgrade --inDir=./.tmp/sample",
    "backup": "deno run -RWE --allow-run ./-scripts/-entry.main.ts --cmd=backup --inDir=./.tmp/sample",
    "help": "deno run -RE ./-scripts/-c.main.ts --cmd=help --inDir=./.tmp/sample",
    "prep": "deno run -RWE ./-scripts/-c.prep.ts",
    "tmp": "deno run -A ./-scripts/-tmp.ts"
  },
  "exports": {
    ".": "./src/mod.ts",
    "./t": "./src/types.ts",
    "./types": "./src/types.ts",
    "./init": "./src/-entry/-init.ts",
    "./main": "./src/-entry/-main.ts",
    "./ui": "./src/ui/mod.ts"
  }
}
