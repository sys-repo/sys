{
  "name": "@sys/driver-vitepress",
  "version": "0.0.23",
  "license": "MIT",
  "tasks": {
    "lint": "deno lint",
    "dry": "deno publish --allow-dirty --dry-run",
    "clean": "deno run -RWE ./-scripts/-clean.ts",
    "test": "deno test -RWNE --allow-run --allow-ffi",

    "dev": "deno run -RWNE --allow-run --allow-ffi ./-scripts/-main.ts --cmd=dev",
    "build": "deno run -RWNE --allow-run --allow-ffi ./-scripts/-main.ts --cmd=build",
    "serve": "deno run -RWNE --allow-run --allow-ffi ./-scripts/-main.ts --cmd=serve"
  },
  "exports": {
    ".": "./src/mod.ts",
    "./t": "./src/types.ts",
    "./types": "./src/types.ts",
    "./init": "./src/-init.ts"
  }
}
